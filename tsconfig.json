{
  "compilerOptions": {
    "target": "es2021",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  
  },
  "include": [ "src/**/*.ts"],
  "exclude": ["node_modules"]
}
